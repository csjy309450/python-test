set(vte_include_dirs /usr/include/vte-2.91 \
                     /usr/include/gtk-3.0 \
                     /usr/include/at-spi2-atk/2.0 \
                     /usr/include/at-spi-2.0 \
                     /usr/include/dbus-1.0 \
                     /usr/lib/x86_64-linux-gnu/dbus-1.0/include \
                     /usr/include/gtk-3.0 \
                     /usr/include/mirclient \
                     /usr/include/mircore \
                     /usr/include/mircookie \
                     /usr/include/cairo \
                     /usr/include/pango-1.0 \
                     /usr/include/harfbuzz \
                     /usr/include/pango-1.0 \
                     /usr/include/atk-1.0 \
                     /usr/include/cairo \
                     /usr/include/pixman-1 \
                     /usr/include/freetype2 \
                     /usr/include/libpng12 \
                     /usr/include/gdk-pixbuf-2.0 \
                     /usr/include/libpng12 \
                     /usr/include/gio-unix-2.0/ \
                     /usr/include/glib-2.0 \
                     /usr/lib/x86_64-linux-gnu/glib-2.0/include \
                     /usr/include/p11-kit-1)
set(vte_libs )
find_library(vte_libs_0 vte-2.91 /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_1 gtk-3  /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_2 gdk-3  /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_3 pangocairo-1.0 /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_4 pango-1.0  /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_5 atk-1.0  /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_6 cairo-gobject /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_7 cairo  /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_8 gdk_pixbuf-2.0 /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_9 gio-2.0 /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_10 gobject-2.0 /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_11 glib-2.0 /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_12 z  /usr/lib/x86_64-linux-gnu)
find_library(vte_libs_13 gnutls /usr/lib/x86_64-linux-gnu)
list(APPEND vte_libs ${vte_libs_0} ${vte_libs_1} ${vte_libs_2} ${vte_libs_3}
${vte_libs_4} ${vte_libs_5} ${vte_libs_6} ${vte_libs_7} ${vte_libs_8} ${vte_libs_9}
${vte_libs_10} ${vte_libs_11} ${vte_libs_12} ${vte_libs_13})
